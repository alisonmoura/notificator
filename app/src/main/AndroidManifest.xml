<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.notificator.notificator">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">

        <activity
            android:name=".activity.MainActivity"
            android:label="Home">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity android:name=".activity.CadastroContatoActivity">
        </activity>

        <activity android:name=".activity.ConfiguracaoContaActivity">
        </activity>
        
        <activity android:name=".activity.CadastroNotificacaoActivity">
        </activity>

    </application>

</manifest>
